#include<stdio.h>
#include<stdlib.h>
#include <locale.h>
typedef struct SNo{
	int chave;
	struct SNo *prox;
}SNo;

SNo *Inicio = NULL;

void insereEntrada(int valor){
	
	SNo *Novo = (SNo *)malloc(sizeof(SNo));
	Novo->chave = valor;
	Novo->prox = Inicio;
	Inicio = Novo;
}
void buscaMaior(int valor){
	SNo *Novo = Inicio;
	int cont=0;
	if(Novo == NULL){
		printf("Lista Vazia \n\n");
		return;
	}
	else{
		while(Novo != NULL){
			if(Novo->chave == valor){
				printf("achei o valor %i! \n\n",valor);
				cont++;
			}
		Novo = Novo->prox;
		}
		if (cont == 0){
			printf("nao achei o valor %i \n\n ",valor);
		}
	}
